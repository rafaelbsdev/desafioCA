{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": 0,
        "no-continue": 0,
        "no-console": 0,
        "no-shadow": 0,
        "no-param-reassign": 0
    }
}
